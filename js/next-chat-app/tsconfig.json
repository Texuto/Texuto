{
	"compilerOptions": {		
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"moduleResolution": "node",
		"noImplicitAny": false
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/hero.jsx"],
	"exclude": ["node_modules"]
}
